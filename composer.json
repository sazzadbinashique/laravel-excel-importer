{
    "name": "sazzadbinashique/laravel-excel-importer",
    "description": "A Laravel package for importing Excel files with preview, validation, progress tracking, and error handling",
    "keywords": [
        "laravel",
        "excel",
        "import",
        "livewire",
        "maatwebsite",
        "validation",
        "queue",
        "progress"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sazzad Bin Ashique",
            "email": "sazzad.sumon35@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SazzadBinAshique\\LaravelExcelImporter\\": "src/",
            "SazzadBinAshique\\LaravelExcelImporter\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SazzadBinAshique\\LaravelExcelImporter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SazzadBinAshique\\LaravelExcelImporter\\ExcelImporterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
